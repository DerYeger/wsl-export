{
  "name": "wsl-export",
  "version": "1.2.10",
  "description": "Interactive CLI for exporting WSL distributions.",
  "repository": "https://github.com/DerYeger/wsl-export.git",
  "author": "Jan MÃ¼ller <janmueller3698@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "eslint --fix --ext \".js,.json,.ts\"",
    "pretty": "prettier --write \"./**/*.{js,json,ts,yml}\"",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "local": "yarn unlink && yarn build && yarn link && wsle"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/*"
  ],
  "bin": {
    "wsle": "./dist/index.js"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "commander": "8.2.0",
    "execa": "5.1.1",
    "figlet": "1.5.2",
    "inquirer": "8.2.0",
    "listr2": "3.12.2"
  },
  "devDependencies": {
    "@types/figlet": "1.5.4",
    "@types/inquirer": "8.1.3",
    "@types/jest": "27.0.2",
    "@types/node": "16.9.1",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "1.1.5",
    "eslint-plugin-vue": "7.19.1",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "tscpaths": "0.0.9",
    "typescript": "4.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
